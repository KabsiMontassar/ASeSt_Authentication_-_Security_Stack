[
  {
    "id": "sample-service",
    "upstream": {
      "url": "http://sample-service:8080"
    },
    "match": {
      "url": "http://localhost:4455/<.*>",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "authenticators": [
      {
        "handler": "jwt"
      }
    ],
    "authorizers": [
      {
        "handler": "allow"
      }
    ],
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  }
]